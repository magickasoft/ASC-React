.profileSectionContainer
  margin 2px 10px 2px 10px
  padding 3px
.profileSectionItem:first-child
  box-shadow inherit
  border inherit
  background-image inherit
.profileSectionItem
  box-shadow inherit
  border inherit
  background-image inherit
  padding initial
.left.profileSectionItem__left
  padding 0 0 0 0
  box-shadow inherit
  border inherit
  background-image inherit
.profileSectionItem__left__icon
  margin-right 10px
  margin-left 10px
.center.profileSectionItem__center
  padding 0 6px 0 0
  overflow hidden
  white-space nowrap
  box-shadow inherit
  border inherit
  background-image inherit
.profileSectionItem__center__inner
  width 100%
.profileSectionItem__center__inner__title
  font-size 16px
  overflow hidden
  text-overflow ellipsis
.right.profileSectionItem__right
  box-shadow inherit
  border inherit
  background-image: inherit
  padding 6px 0 6px 0
.profileSectionItem__right__inner
  background-color #9e1c26
  margin-right 2px
  width 28px
  height 28px
  border-radius 50%
  display flex
  justify-content center
  align-items center
.profileSectionItem__right__icon
  transform scale(1.2)
.profileSectionItem__left.list__item__left.list__item--material__left
  min-width 0

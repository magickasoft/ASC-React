.sectionInboxContainer
  background rgba(0,0,0,0.25)
  padding 0
.sectionInboxList__left__status
  right 4px
  bottom 4px
  position absolute
  width 15px
  height 15px
  background-color green
  border-radius 50%
.sectionInboxList:first-child
  //box-shadow inherit
  border inherit
  background-image inherit
.sectionInboxList
  //box-shadow inherit
  border inherit
  background-image inherit
  padding initial
.left.sectionInboxList__left
  padding 10px 8px 10px 8px
  //box-shadow inherit
  //border inherit
  //background-image inherit
  position relative
  //border: 1px solid #424242;
  //border-top: none;
  //border-left: none;
  //border-right: none;
.sectionInboxList__left__icon
  width 50px
  height 50px
  background-repeat no-repeat
  background-position 50%
  border-radius 5px
  border 1px solid #9a8f8f
.center.sectionInboxList__center
  padding 12px 6px 12px 6px
  overflow hidden
  white-space nowrap
  //box-shadow inherit
  //border inherit
  //background-image inherit
  //border: 1px solid #424242;
  //border-top: none;
  //border-left: none;
  //border-right: none;
.sectionInboxList__center__title
  font-size 16px
  overflow hidden
  text-overflow ellipsis
.sectionInboxList__center__label
  font-size 14px
  color #7c7c7c
  overflow hidden
  text-overflow ellipsis
.right.sectionInboxList__right
  //box-shadow inherit
  //border inherit
  background-image inherit
  padding: 12px 0 12px 0
  display flex
  flex-direction column
  align-items flex-start
  //border: 1px solid #424242;
  //border-top: none;
  //border-left: none;
  //border-right: none;
.sectionInboxList__right__iconBlock
  margin-right 10px
  margin-top 3px
  font-size 16px
  opacity 0.85
.sectionInboxList__left.list__item__left.list__item--material__left
  min-width 72px
.sectionInboxList.list__item--material:first-child
  box-shadow inset 0 1px 0 0 #f1f1f1
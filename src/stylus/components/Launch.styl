.launchContainer
  height 100%
  min-height 100%
  display flex
  justify-content center
  width 100%
.launchContainer_buttons
  display flex
  justify-content center
  flex-direction row
  align-items flex-end
  width 100%
  position absolute
  bottom 0
  left 0
.launchContainer_titles
  display flex
  justify-content center
  flex-direction column
  align-items flex-start
  font-weight 600
  padding-left 20px
  padding-right 20px
.launchContainer_titles_main
  font-size 42px
  line-height 42px
  color #fff
.launchContainer_titles_added
  font-size 75px
  line-height 85px
  color #fff